@model SocialWeb.ViewModels.HomeIndex
@{
    ViewData["Title"] = "SocialWeb";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!DOCTYPE html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - SocialWeb</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/SocialWeb.styles.css" asp-append-version="true" />

    <script src="https://kit.fontawesome.com/e50213ec74.js" crossorigin="anonymous"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"></script>
</head>
<body>
    <div id="friendRequestContainer"style="width: 70%; margin: 0 auto; background-color: #FF4E88">
        @Html.Partial("_FriendRequestPartial", Model.FriendRequest)
    </div>
    <a asp-controller="User" asp-action="Index">Personal Page</a>
</body>
